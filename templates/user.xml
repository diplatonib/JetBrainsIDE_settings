    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="ar" value="&lt;p&gt;&#10;    + $END$&lt;br&gt;&#10;    &lt;a href=&quot;$CLIPBOARD$&quot; title=&quot;regular&quot; style=&quot;color: #000; background-color: #a3abdb;&quot;&gt;$TEXT$&lt;/a&gt;&#10;&lt;/p&gt;" description="Regular task" toReformat="false" toShortenFQNames="true">
    <variable name="CLIPBOARD" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="gtv" value="{{ site._(&quot;$CLIPBOARD_TEXT$&quot;)|replace_vars($END$)}}" description="Gittext wrapper long" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD_TEXT" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="gt" value="{{ site._(&quot;$CLIPBOARD$&quot;) }}" description="Gittext wrapper short" toReformat="true" toShortenFQNames="true">
