<templateSet group="user">
  <template name="vn" value="{{ settings.vendor_name }}" description="TrueConf as {{ settings.vendor_name }} twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="bu" value="{{ settings.blog_url }}" description="TC_WEB blog url twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="HTML_TEXT" value="false" />
    </context>
  </template>
  <template name="tcv" value="{{ settings.videobar_name }}" description="TrueConf Videobar as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="sln" value="{{ site.link($CLIPBOARD_ID$$END$) }}" description="TC_WEB sitemap link by ID" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD_ID" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="HTML" value="true" />
      <option name="HTML_TEXT" value="false" />
    </context>
  </template>
  <template name="tce" value="{{ settings.enterprise_name }}" description="TrueConf Enterprise as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcr" value="{{ settings.room_name }}" description="TrueConf Room as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcg" value="{{ settings.group_name }}" description="TrueConf Group as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tct" value="{{ settings.terminal_name }}" description="TrueConf Group as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcsf" value="{{ settings.server_name_free }}" description="TrueConf Server Free as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="day" value="## $DATE$&#10;&#10;$END$" description="Report date" toReformat="true" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="mr" value="#### Merge request:&#10;$CLIPBOARD$&#10;#### Task:" description="Merge request" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="ar" value="&lt;p&gt;+ $END$&lt;br&gt;&#10;&lt;a href=&quot;$CLIPBOARD$&quot; title=&quot;regular&quot; style=&quot;color: #000; background-color: #a3abdb;&quot;&gt;$TEXT$&lt;/a&gt;&lt;/p&gt;" description="Regular task" toReformat="false" toShortenFQNames="true">
    <variable name="CLIPBOARD" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="gtv" value="{{ site._(&quot;$CLIPBOARD_TEXT$&quot;)|replace_vars($END$)}}" description="Gittext wrapper long" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD_TEXT" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="gt" value="{{ site._(&quot;$CLIPBOARD_TEXT$&quot;) }}" description="Gittext wrapper short" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD_TEXT" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="rv" value="|replace_vars({&quot;$END$&quot; : $CLIPBOARD_VARIABLE$})" description="Replace vars twig filter" toReformat="true" toShortenFQNames="true">
    <variable name="CLIPBOARD_VARIABLE" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tco" value="{{ settings.cloud_name }}" description="TrueConf Online as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcm" value="{{ settings.mcu_name }}" description="TrueConf MCU as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tctr" value="{{ settings.tracker_name }}" description="TrueConf Tracker as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcw" value="{{ settings.weathervane_name }}" description="TrueConf Weathervane as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tck" value="{{ settings.kiosk_name }}" description="TrueConf Kiosk as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cvn" value="{{ compared_vendor_name }}" description="{{ compared_vendor_name }} variable for landings" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="tcs" value="{{ settings.server_name }}" description="TrueConf Server as twig variable" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="pic" value="&lt;picture&gt;&#10;    &lt;source data-srcset=&quot;$AVIF$&quot; type=&quot;image/avif&quot;&gt;&#10;    &lt;source data-srcset=&quot;$WEBP$&quot; type=&quot;image/webp&quot;&gt;&#10;    $SELECTION$&#10;&lt;/picture&gt;" description="Picture snippet for avif/webp optimizerd images" toReformat="true" toShortenFQNames="true">
    <variable name="AVIF" expression="regularExpression(clipboard(), &quot;.ipg$|.jpg$|.png$&quot;, &quot;.avif&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="WEBP" expression="regularExpression(clipboard(), &quot;.ipg$|.jpg$|.png$&quot;, &quot;.webp&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="HAML" value="false" />
      <option name="HTML" value="false" />
      <option name="HTML_TEXT" value="true" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="PHP" value="false" />
      <option name="REQUEST" value="false" />
      <option name="SHELL_SCRIPT" value="false" />
      <option name="SQL" value="false" />
      <option name="TypeScript" value="false" />
      <option name="Vue" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
  <template name="pic_re" value="&lt;picture&gt;&#10;    &lt;source srcset=&quot;$AVIF$&quot; type=&quot;image/avif&quot;&gt;&#10;    &lt;source srcset=&quot;$WEBP$&quot; type=&quot;image/webp&quot;&gt;&#10;    $SELECTION$&#10;&lt;/picture&gt;" description="Picture snippet with regex for avif/webp optimizerd images" toReformat="true" toShortenFQNames="true">
    <variable name="AVIF" expression="regularExpression(SELECTION, &quot;(src=\&quot;(.*?)\&quot;)&quot;, &quot;$2.avif&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="WEBP" expression="regularExpression(clipboard(), &quot;.ipg$|.jpg$|.png$&quot;, &quot;.webp&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="HAML" value="false" />
      <option name="HTML" value="false" />
      <option name="HTML_TEXT" value="true" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="PHP" value="false" />
      <option name="REQUEST" value="false" />
      <option name="SHELL_SCRIPT" value="false" />
      <option name="SQL" value="false" />
      <option name="TypeScript" value="false" />
      <option name="Vue" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
</templateSet>